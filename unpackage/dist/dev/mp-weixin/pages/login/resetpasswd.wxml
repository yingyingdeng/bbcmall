<view><view class="form"><view class="code"><input class="phone-input" placeholder="请输入手机号" data-event-opts="{{[['input',[['__set_model',['','phone','$event',[]]]]]]}}" value="{{phone}}" bindinput="__e"/><view class="phone-num"><picker value="{{index}}" range="{{phonenum}}" data-event-opts="{{[['change',[['PhoneChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(index>-1?phonenum[index]:'+86')}}<text class="icon iconfont icon-jiantouxia"></text></view></picker></view></view><view class="password"><input class="forget-input" placeholder="请输入手机验证码" data-event-opts="{{[['input',[['__set_model',['','checknum','$event',[]]]]]]}}" value="{{checknum}}" bindinput="__e"/><view class="wjmi"><view>获取验证码</view></view></view><view class="password"><input class="forget-input" placeholder="请设置新密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/></view><button class="{{['btn',(disabled)?'btn-disabled':'']}}" type="warn" loading="{{loading}}" disabled="{{disabled}}" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">立即注册</button><view class="res"><view data-event-opts="{{[['tap',[['toLogin',['$event']]]]]}}" bindtap="__e">已有帐号立即登录</view></view></view></view>