page {
  background-color: #f1f1f1;
  font-size: 30rpx;
}
.headerbg {
  background: url(data:image/jpeg;base64,) no-repeat;
  width: 100%;
  height: 160rpx;
}
.integral {
  height: 160rpx;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
      -ms-flex-pack: center;
          justify-content: center;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
      -ms-flex-direction: column;
          flex-direction: column;
}
.integral text {
  color: #f1f1f1;
  font-size: 60rpx;
  line-height: 1;
}
.integral .jifengz {
  color: #f1f1f1;
  font-size: 28rpx;
  padding-top: 15rpx;
}
.icon-gantanhao-yuankuang {
  font-size: 24rpx;
  margin-right: 15rpx;
}
.jifen {
  background: #fff;
  padding: 10rpx 30rpx;
  border-bottom: 1rpx #f3f3f3 solid;
}
.jftext {
  font-size: 32rpx;
}
.jftimes {
  font-size: 26rpx;
  color: #aaa;
}
.jfnum {
