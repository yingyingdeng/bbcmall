
page{background: #f3f3f3;}
.duihuan{background: #fff;margin: 20rpx 2% 0 2%;border-radius: 20rpx;padding: 20rpx 3%;color: #797979;}
.lipinimg{width: 130rpx;padding-right: 20rpx;}
.lipinimg image{width: 130rpx; height: 130rpx;border-radius: 10rpx;}
.jifen{margin-top: 20rpx;}
.lptitle{display: -webkit-box;-webkit-box-orient: vertical;overflow: hidden;-webkit-line-clamp: 1;font-size: 30rpx; color: #333; line-height: 1.5;}
.icon-shijian{margin-right: 10rpx;font-size: 28rpx;}
.chakan{background: -webkit-gradient(linear, left top, right top, from(#ff4672) , to(#fe3612));background: -o-linear-gradient(left, #ff4672 , #fe3612);background: linear-gradient(to right, #ff4672 , #fe3612);padding: 10rpx 20rpx;line-height: 1.5;color: #fff;border-radius: 10rpx;font-size: 24rpx;}
.quxiao{background: #f1f1f1;color: #333333;padding: 10rpx 20rpx;line-height: 1.5;border-radius: 5rpx;font-size: 24rpx;margin-left: 20rpx;}
.red{color: #C9141D;}
.danhao{color: #333333;}
.green{color: #1AAD19;}
.shouhuo{display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;-webkit-box-align: center;-webkit-align-items: center;-ms-flex-align: center;align-items: center;-webkit-box-pack: end;-webkit-justify-content: flex-end;-ms-flex-pack: end;justify-content: flex-end;}
.dingdan{background: url(data:image/jpeg;base64,) repeat-x bottom #fff;background-size:100rpx;border-radius: 0 0 20rpx 20rpx;padding: 30rpx 30rpx 30rpx 10rpx;color: #333;min-height: 80rpx;}
.address{ padding-left: 20rpx}
.city{font-size: 32rpx}
.city text{background: #f52d19; border-radius: 5rpx; height: 32rpx; font-size: 26rpx; color: #f3f3f3; margin-right: 15rpx;line-height: 32rpx; padding: 0rpx 10rpx;}
.add{font-size: 36rpx;font-weight: bold;}
.name,.tel{font-size: 30rpx; margin-right: 30rpx;}
.msgtitle{font-size: 32rpx;font-weight: bold;color: #333;}
.msgtitle text{border-left: 5rpx solid #c9141d;padding-left:15rpx;line-height: 2}
.xinxi{padding: 20rpx}
.xinxi>view{padding: 10rpx 0;}
.kuaidi{background: #f9f9f9;color: #333;padding: 20rpx; border-radius: 10rpx;margin: 15rpx 0;}


